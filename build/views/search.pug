extends layouts/main
include mixins/videoBlock

block content
    .search__header
        h3 Searching for : #{searchingBy}
    .search__videos
        if videoList.length === 0 
            h5 Video not founded
        each video in videoList
            +videoBlock({
                title:video.title,
                views:video.views,
                videoFile:video.videoFile,
                id:video.id,
            })